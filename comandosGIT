git clone "URL" //Realiza un repositorio en la nube y clona

git remote add nombre URL //Cambias de nombre(asocias nombre a URL)

git fetch "nombre" //Trae informacion sobre estado,ramas...

git pull //Combina trabajos de repositorio

git push origin //Envia datos al repositorio

git remote show [nombre-remoto] //Inspeccionar informacion de un remoto concreto

git remote rm //Eliminar remotos

git remote rename "nombre_antiguo" "nombre_nuevo" //Renombrar remotos
